{
  "name": "ReactNativeTypescript",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:and": "NODE_ENV=development && react-native run-android",
    "dev:ios": "NODE_ENV=development && react-native run-ios",
    "test": "jest",
    "lint": "eslint . --ext .js, .jsx, .ts, .tsx",
    "clean:yarn": "yarn cache clean && npm cache clean --force",
    "clean:metro": "yarn react-native start --reset-cache",
    "pod": "cd ./ios && pod install && cd ..",
    "reset": "watchman watch-del-all && sudo rm -rf node_modules && sudo yarn install && sudo rm -rf /tmp/metro-*",
    "clean": "npm run clean:yarn && npm run pod && npm run reset && npm run clean:metro",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "apk": "cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.5.2",
    "@react-navigation/drawer": "^5.8.4",
    "@react-navigation/material-bottom-tabs": "^5.2.12",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.6.2",
    "@types/styled-components": "^5.1.0",
    "axios": "^0.19.2",
    "moment": "^2.27.0",
    "react": "16.11.0",
    "react-hook-form": "^6.0.8",
    "react-native": "0.62.2",
    "react-native-easy-toast": "^1.2.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-picker": "^2.3.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-paper": "^3.10.1",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.0.6",
    "react-native-screens": "^2.9.0",
    "react-native-tiny-toast": "^1.0.7",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^10.3.2",
    "styled-components": "^5.1.1",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@bam.tech/react-native-make": "^3.0.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.38",
    "@types/react-native": "^0.62.13",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.5.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "ts-jest": {
        "tsConfig": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
